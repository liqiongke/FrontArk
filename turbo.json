{
  "$schema": "https://turborepo.org/schema.json",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        "lib/**"
      ],
      "cache": true
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "dev:mock": {
      // "dependsOn": [
      //   "^dev:mock"
      // ],
      "cache": false,
      "persistent": true
    },
    "lint": {
      "cache": true
    }
  }
}